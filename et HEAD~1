[1m--- /dev/null[m
[1m+++ b/src/geoserver/data/demo/WFS_transactionDelete.xml[m
[36m@@ -0,0 +1,20 @@[m
[32m+[m[32m<!-- We include this transaction as a sample for deletes.        -->[m
[32m+[m[32m<!--[m
[32m+[m[32m      YOU PROBABLY DO NOT WANT TO RUN THIS QUERY SINCE[m[41m [m
[32m+[m[32m       IT WILL MODIFY YOUR SOURCE DATA FILES[m
[32m+[m[32m  -->[m
[32m+[m[32m<wfs:Transaction service="WFS" version="1.0.0"[m
[32m+[m[32m  xmlns:cdf="http://www.opengis.net/cite/data"[m
[32m+[m[32m  xmlns:ogc="http://www.opengis.net/ogc"[m
[32m+[m[32m  xmlns:wfs="http://www.opengis.net/wfs"[m
[32m+[m[32m  xmlns:topp="http://www.openplans.org/topp">[m
[32m+[m[32m  <wfs:Delete typeName="topp:tasmania_roads">[m
[32m+[m[32m    <ogc:Filter>[m
[32m+[m[32m      <ogc:PropertyIsEqualTo>[m
[32m+[m[32m        <ogc:PropertyName>topp:TYPE</ogc:PropertyName>[m
[32m+[m[32m        <ogc:Literal>alley</ogc:Literal>[m
[32m+[m[32m      </ogc:PropertyIsEqualTo>[m
[32m+[m[32m    </ogc:Filter>[m
[32m+[m[32m  </wfs:Delete>[m
[32m+[m[32m</wfs:Transaction>[m
[32m+[m[41m [m
