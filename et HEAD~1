[36m@@ -0,0 +1,21 @@[m
[32m+[m[32m<?xml version="1.0" encoding="UTF-8"?>[m
[32m+[m[32m<!-- Demonstrates use of WCS scale factor -->[m
[32m+[m[32m<wcs:GetCoverage service="WCS" version="2.0.1"[m
[32m+[m[32m    xmlns:wcs="http://www.opengis.net/wcs/2.0" xmlns:scal="http://www.opengis.net/WCS_service-extension_scaling/1.0"[m
[32m+[m[32m    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"[m
[32m+[m[32m    xsi:schemaLocation="http://www.opengis.net/wcs/2.0 http://schemas.opengis.net/wcs/2.0/wcsAll.xsd">[m
[32m+[m[32m    <!--[m
[32m+[m[32m        The full schemaLocation permits schema-validation but causes the[m
[32m+[m[32m        request to fail [GEOS-7779]:[m
[32m+[m[32m        xsi:schemaLocation="http://www.opengis.net/wcs/2.0 http://schemas.opengis.net/wcs/2.0/wcsAll.xsd[m
[32m+[m[32m        http://www.opengis.net/WCS_service-extension_scaling/1.0 http://schemas.opengis.net/wcs/scaling/1.0/wcsScal.xsd">[m
[32m+[m[32m    -->[m
[32m+[m[32m    <wcs:Extension>[m
[32m+[m[32m        <scal:Scaling>[m
[32m+[m[32m            <scal:ScaleByFactor>[m
[32m+[m[32m                <scal:scaleFactor>0.5</scal:scaleFactor>[m
[32m+[m[32m            </scal:ScaleByFactor>[m
[32m+[m[32m        </scal:Scaling>[m
[32m+[m[32m    </wcs:Extension>[m
[32m+[m[32m    <wcs:CoverageId>nurc__mosaic</wcs:CoverageId>[m
[32m+[m[32m</wcs:GetCoverage>[m
