[36m@@ -0,0 +1,28 @@[m
[32m+[m[32m<!-- Performs a get feature with a bounding box filter.      -->[m
[32m+[m[32m<!-- The BBOX filter is a convenience for a <Not><Disjoint>, -->[m
[32m+[m[32m<!-- it fetches all features that spatially interact with the given box. -->[m
[32m+[m[32m<!-- This example also shows how to request specific properties, in this -->[m
[32m+[m[32m<!-- case we just get the STATE_NAME and PERSONS -->[m
[32m+[m
[32m+[m[32m<wfs:GetFeature service="WFS" version="1.0.0"[m
[32m+[m[32m  outputFormat="GML2"[m
[32m+[m[32m  xmlns:topp="http://www.openplans.org/topp"[m
[32m+[m[32m  xmlns:wfs="http://www.opengis.net/wfs"[m
[32m+[m[32m  xmlns:ogc="http://www.opengis.net/ogc"[m
[32m+[m[32m  xmlns:gml="http://www.opengis.net/gml"[m
[32m+[m[32m  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"[m
[32m+[m[32m  xsi:schemaLocation="http://www.opengis.net/wfs[m
[32m+[m[32m                      http://schemas.opengis.net/wfs/1.0.0/WFS-basic.xsd">[m
[32m+[m[32m  <wfs:Query typeName="topp:states">[m
[32m+[m[32m    <ogc:PropertyName>topp:STATE_NAME</ogc:PropertyName>[m
[32m+[m[32m    <ogc:PropertyName>topp:PERSONS</ogc:PropertyName>[m
[32m+[m[32m    <ogc:Filter>[m
[32m+[m[32m      <ogc:BBOX>[m
[32m+[m[32m        <ogc:PropertyName>the_geom</ogc:PropertyName>[m
[32m+[m[32m        <gml:Box srsName="http://www.opengis.net/gml/srs/epsg.xml#4326">[m
[32m+[m[32m           <gml:coordinates>-75.102613,40.212597 -72.361859,41.512517</gml:coordinates>[m
[32m+[m[32m        </gml:Box>[m
[32m+[m[32m      </ogc:BBOX>[m
[32m+[m[32m   </ogc:Filter>[m
[32m+[m[32m  </wfs:Query>[m
[32m+[m[32m</wfs:GetFeature>[m
