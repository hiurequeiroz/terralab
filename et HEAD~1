[1mdiff --git a/src/geoserver/data/demo/WCS_getCoverage.xml b/src/geoserver/data/demo/WCS_getCoverage.xml[m
[1mnew file mode 100644[m
[1mindex 0000000..f74b053[m
[1m--- /dev/null[m
[1m+++ b/src/geoserver/data/demo/WCS_getCoverage.xml[m
[36m@@ -0,0 +1,38 @@[m
[32m+[m[32m<!-- Performs a get coverage.  Feel free to play with the elements,        -->[m
[32m+[m[32m<!-- to get different results.                                             -->[m
[32m+[m[32m<GetCoverage service="WCS" version="1.0.0"[m
[32m+[m[32m  xmlns="http://www.opengis.net/wcs"[m
[32m+[m[32m  xmlns:nurc="http://www.nurc.nato.int"[m
[32m+[m[32m  xmlns:ogc="http://www.opengis.net/ogc"[m
[32m+[m[32m  xmlns:gml="http://www.opengis.net/gml"[m
[32m+[m[32m  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"[m
[32m+[m[32m  xsi:schemaLocation="http://www.opengis.net/wcs http://schemas.opengis.net/wcs/1.0.0/getCoverage.xsd">[m
[32m+[m[32m<sourceCoverage>nurc:Pk50095</sourceCoverage>[m
[32m+[m[32m  <domainSubset>[m
[32m+[m[32m    <spatialSubset>[m
[32m+[m[32m      <gml:Envelope srsName="EPSG:32633">[m
[32m+[m[32m        <gml:pos>347649.93086859107 5176214.082539256</gml:pos>[m
[32m+[m[32m        <gml:pos>370725.976428591 5196961.352859256</gml:pos>[m
[32m+[m[32m      </gml:Envelope>[m
[32m+[m[32m      <gml:Grid dimension="2" srsName="EPSG:4326">[m
[32m+[m[32m        <gml:limits>[m
[32m+[m[32m          <gml:GridEnvelope>[m
[32m+[m[32m            <gml:low>0 0</gml:low>[m
[32m+[m[32m            <gml:high>545 490</gml:high>[m
[32m+[m[32m          </gml:GridEnvelope>[m
[32m+[m[32m        </gml:limits>[m
[32m+[m[32m        <gml:axisName>E</gml:axisName>[m
[32m+[m[32m        <gml:axisName>N</gml:axisName>[m
[32m+[m[32m      </gml:Grid>[m
[32m+[m[32m    </spatialSubset>[m
[32m+[m[32m  </domainSubset>[m
[32m+[m[32m  <rangeSubset>[m
[32m+[m[32m    <axisSubset name="Band">[m
[32m+[m[32m      <singleValue>1</singleValue>[m
[32m+[m[32m    </axisSubset>[m
[32m+[m[32m  </rangeSubset>[m
[32m+[m[32m  <output>[m
[32m+[m[32m      <crs>EPSG:32633</crs>[m
[32m+[m[32m      <format>GeoTIFF</format>[m
[32m+[m[32m  </output>[m
[32m+[m[32m</GetCoverage>[m
