[32m+[m[32m<!--[m
[32m+[m[32m      YOU PROBABLY DO NOT WANT TO RUN THIS QUERY SINCE[m[41m [m
[32m+[m[32m       IT WILL MODIFY YOUR SOURCE DATA FILES[m
[32m+[m[41m       [m
[32m+[m[32m       It will add a simple line to the tasmania_roads dataset.[m
[32m+[m[41m       [m
[32m+[m[32m  -->[m
[32m+[m[32m<wfs:Transaction service="WFS" version="1.0.0"[m
[32m+[m[32m  xmlns:wfs="http://www.opengis.net/wfs"[m
[32m+[m[32m  xmlns:topp="http://www.openplans.org/topp"[m
[32m+[m[32m  xmlns:gml="http://www.opengis.net/gml"[m
[32m+[m[32m  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"[m
[32m+[m[32m  xsi:schemaLocation="http://www.opengis.net/wfs http://schemas.opengis.net/wfs/1.0.0/WFS-transaction.xsd http://www.openplans.org/topp http://localhost:8080/geoserver/wfs/DescribeFeatureType?typename=topp:tasmania_roads">[m
[32m+[m[32m  <wfs:Insert>[m
[32m+[m[32m    <topp:tasmania_roads>[m
[32m+[m[32m      <topp:the_geom>[m
[32m+[m[32m        <gml:MultiLineString srsName="http://www.opengis.net/gml/srs/epsg.xml#4326">[m
[32m+[m[32m          <gml:lineStringMember>[m
[32m+[m[32m            <gml:LineString>[m
[32m+[m[32m              <gml:coordinates decimal="." cs="," ts=" ">[m
[32m+[m[32m494475.71056415,5433016.8189323 494982.70115662,5435041.95096618[m
[32m+[m[32m              </gml:coordinates>[m
[32m+[m[32m            </gml:LineString>[m
[32m+[m[32m          </gml:lineStringMember>[m
[32m+[m[32m        </gml:MultiLineString>[m
[32m+[m[32m      </topp:the_geom>[m
[32m+[m[32m      <topp:TYPE>alley</topp:TYPE>[m
[32m+[m[32m    </topp:tasmania_roads>[m
[32m+[m[32m  </wfs:Insert>[m
[32m+[m[32m</wfs:Transaction>[m
