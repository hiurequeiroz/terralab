[32m+[m[32m<?xml version="1.0" encoding="UTF-8"?>[m
[32m+[m[32m<!--[m
[32m+[m[32m    This example demonstrates a WFS 2.0 GetFeature POST request.[m
[32m+[m
[32m+[m[32m    This filter selects a single feature with id "bugsites.3".[m
[32m+[m
[32m+[m[32m    See also:[m
[32m+[m[32m    WFS Standard: http://www.opengeospatial.org/standards/wfs[m
[32m+[m[32m    Filter Encoding Standard: http://www.opengeospatial.org/standards/filter[m
[32m+[m[32m-->[m
[32m+[m[32m<wfs:GetFeature service="WFS" version="2.0.0"[m
[32m+[m[32m    xmlns:wfs="http://www.opengis.net/wfs/2.0" xmlns:fes="http://www.opengis.net/fes/2.0"[m
[32m+[m[32m    xmlns:sf="http://www.openplans.org/spearfish" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"[m
[32m+[m[32m    xsi:schemaLocation="http://www.opengis.net/wfs/2.0 http://schemas.opengis.net/wfs/2.0/wfs.xsd">[m
[32m+[m[32m    <wfs:Query typeNames="sf:bugsites">[m
[32m+[m[32m        <fes:Filter>[m
[32m+[m[32m            <fes:ResourceId rid="bugsites.3"/>[m
[32m+[m[32m        </fes:Filter>[m
[32m+[m[32m    </wfs:Query>[m
[32m+[m[32m</wfs:GetFeature>[m
