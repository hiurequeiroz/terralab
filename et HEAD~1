[1m+++ b/src/geoserver/data/cluster/embedded-broker.properties[m
[36m@@ -0,0 +1,37 @@[m
[32m+[m[32m## JMX settings (can be overridden by env vars)[m
[32m+[m[32m## For more information, see: http://activemq.apache.org/jmx.html[m
[32m+[m[32m# enable/disable broker jmx[m
[32m+[m[32mactivemq.jmx.useJmx=false[m
[32m+[m[32m# set the JMX connector port[m
[32m+[m[32mactivemq.jmx.port=1098[m
[32m+[m[32m# set the JMX connector host[m
[32m+[m[32mactivemq.jmx.host=localhost[m
[32m+[m[32m# enable the JMX connector[m
[32m+[m[32mactivemq.jmx.createConnector=false[m
[32m+[m
[32m+[m[32m## broker settings[m
[32m+[m[32m# set the base path of the temporary broker dir[m
[32m+[m[32m# this is also used as persistence base dir[m[41m  [m
[32m+[m[32m#activemq.base=./[m
[32m+[m
[32m+[m[32m## configuring the embedded broker[m
[32m+[m[32m# connects the server via native I/O socket to the local network broadcast using an automatic assigned port[m[41m [m
[32m+[m[32m# maximumConnections and wireFormat.maxFrameSize are added to avoid ddos attacks[m
[32m+[m[32mactivemq.transportConnectors.server.uri=nio://127.0.0.1:0?maximumConnections=1000&wireFormat.maxFrameSize=104857600&jms.useAsyncSend=true&transport.daemon=true&trace=true[m
[32m+[m
[32m+[m[32m## the broker performs discovery using the following protocol:[m
[32m+[m[32m# multicast on default network[m
[32m+[m[32mactivemq.transportConnectors.server.discoveryURI=multicast://default[m
[32m+[m
[32m+[m[32m## persistence settings and system usage[m
[32m+[m[32m## The systemUsage controls the maximum amount of space the broker will[m[41m [m
[32m+[m[32m## use before slowing down producers.[m[41m [m
[32m+[m[32m## For more information, see: http://activemq.apache.org/producer-flow-control.html[m
[32m+[m[32m# enable/disable persistence[m
[32m+[m[32mactivemq.broker.persistent=true[m
[32m+[m[32m# heap memory usage[m
[32m+[m[32mactivemq.broker.systemUsage.memoryUsage=128 mb[m
[32m+[m[32m# disk space memory usage[m
[32m+[m[32mactivemq.broker.systemUsage.storeUsage=1 gb[m
[32m+[m[32m# temp disk space memory usage[m
[32m+[m[32mactivemq.broker.systemUsage.tempUsage=128 mb[m
[1mdiff --git a/src/geoserver/data/controlflow.properties b/src/geoserver/data/controlflow.properties[m
[1mnew file mode 100644[m
[1mindex 0000000..3acd7d7[m
[1m--- /dev/null[m
[1m+++ b/src/geoserver/data/controlflow.properties[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32m# if a request waits in queue for more than 60 seconds it's not worth executing,[m
[32m+[m[32m# the client will  likely have given up by then[m
[32m+[m[32mtimeout=60[m
[32m+[m[32m# don't allow the execution of more than 100 requests total in parallel[m
[32m+[m[32mows.global=100[m
[32m+[m[32m# don't allow more than 10 GetMap in parallel[m
[32m+[m[32mows.wms.getmap=10[m
[32m+[m[32m# don't allow more than 4 outputs with Excel output as it's memory bound[m
[32m+[m[32mows.wfs.getfeature.application/msexcel=4[m
[32m+[m[32m# don't allow a single user to perform more than 6 requests in parallel[m
[32m+[m[32m# (6 being the Firefox default concurrency level at the time of writing)[m
[32m+[m[32muser=6[m
[32m+[m[32m# don't allow the execution of more than 16 tile requests in parallel[m
[32m+[m[32m# (assuming a server with 4 cores, GWC empirical tests show that throughput[m
[32m+[m[32m# peaks up at 4 x number of cores. Adjust as appropriate to your system)[m
[32m+[m[32mows.gwc=16[m
\ No newline at end of file[m
