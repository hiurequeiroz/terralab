[1mindex 0000000..84a254a[m
[1m--- /dev/null[m
[1m+++ b/src/geoserver/data/demo/WFS_getFeatureIntersects-1.0.xml[m
[36m@@ -0,0 +1,34 @@[m
[32m+[m[32m<!-- Performs an intersects against a point.  This is functionally -->[m
[32m+[m[32m<!-- equivalent to <Not><Disjoint>.  This call can be used by a    -->[m
[32m+[m[32m<!-- client application to select a feature clicked on.[m[41m  [m
[32m+[m
[32m+[m[32m     This will search through the dataset and return any polygons that[m
[32m+[m[32m     contain the search point.[m[41m  [m
[32m+[m[41m     [m
[32m+[m[32m     If you were searching in a point or line dataset, you might want[m
[32m+[m[32m     to make a little polygon to search with instead of a single point[m
[32m+[m[32m     so the user doesnt have to *exactly* click on the (mathematically[m
[32m+[m[32m     infinitely thin) line or point.[m[41m     [m
[32m+[m
[32m+[m[32m -->[m
[32m+[m[32m<wfs:GetFeature service="WFS" version="1.0.0"[m
[32m+[m[32m  outputFormat="GML2"[m
[32m+[m[32m  xmlns:topp="http://www.openplans.org/topp"[m
[32m+[m[32m  xmlns:wfs="http://www.opengis.net/wfs"[m
[32m+[m[32m  xmlns="http://www.opengis.net/ogc"[m
[32m+[m[32m  xmlns:gml="http://www.opengis.net/gml"[m
[32m+[m[32m  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"[m
[32m+[m[32m  xsi:schemaLocation="http://www.opengis.net/wfs[m
[32m+[m[32m                      http://schemas.opengis.net/wfs/1.0.0/WFS-basic.xsd">[m
[32m+[m[32m  <wfs:Query typeName="topp:states">[m
[32m+[m[32m    <Filter>[m
[32m+[m[32m      <Intersects>[m
[32m+[m[32m        <PropertyName>the_geom</PropertyName>[m
[32m+[m[32m          <gml:Point srsName="http://www.opengis.net/gml/srs/epsg.xml#4326">[m
[32m+[m[32m            <gml:coordinates>-74.817265,40.5296504</gml:coordinates>[m
[32m+[m[32m          </gml:Point>[m
[32m+[m[32m        </Intersects>[m
[32m+[m[32m      </Filter>[m
[32m+[m[32m  </wfs:Query>[m
[32m+[m[32m</wfs:GetFeature>[m
[32m+[m[41m [m
[1mdiff --git a/src/geoserver/data/demo/WFS_getFeatureIntersects-1.1.xml b/src/geoserver/data/demo/WFS_getFeatureIntersects-1.1.xml[m
