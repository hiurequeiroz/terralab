[1mdiff --git a/src/geoserver/data/demo/WFS_getFeatureNotDisjoint-2.0.xml b/src/geoserver/data/demo/WFS_getFeatureNotDisjoint-2.0.xml[m
[1mnew file mode 100644[m
[1mindex 0000000..520d320[m
[1m--- /dev/null[m
[1m+++ b/src/geoserver/data/demo/WFS_getFeatureNotDisjoint-2.0.xml[m
[36m@@ -0,0 +1,42 @@[m
[32m+[m[32m<?xml version="1.0" encoding="UTF-8"?>[m
[32m+[m[32m<!--[m
[32m+[m[32m    This example demonstrates a WFS 2.0 GetFeature POST request.[m
[32m+[m
[32m+[m[32m    WFS 2.0 does not depend on any one GML version and thus[m
[32m+[m[32m    requires an explicit namespace and schemaLocation for GML.[m
[32m+[m
[32m+[m[32m    This spatial filter selects a single feature with[m
[32m+[m[32m    gml:id="bugsites.2".[m
[32m+[m
[32m+[m[32m    See also:[m
[32m+[m[32m    WFS Standard: http://www.opengeospatial.org/standards/wfs[m
[32m+[m[32m    Filter Encoding Standard: http://www.opengeospatial.org/standards/filter[m
[32m+[m[32m-->[m
[32m+[m[32m<wfs:GetFeature service="WFS" version="2.0.0"[m
[32m+[m[32m    xmlns:wfs="http://www.opengis.net/wfs/2.0" xmlns:fes="http://www.opengis.net/fes/2.0"[m
[32m+[m[32m    xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:sf="http://www.openplans.org/spearfish"[m
[32m+[m[32m    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"[m
[32m+[m[32m    xsi:schemaLocation="http://www.opengis.net/wfs/2.0 http://schemas.opengis.net/wfs/2.0/wfs.xsd[m
[32m+[m[32m        http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/gml.xsd">[m
[32m+[m[32m    <wfs:Query typeNames="sf:bugsites">[m
[32m+[m[32m        <fes:Filter>[m
[32m+[m[32m            <fes:Not>[m
[32m+[m[32m                <fes:Disjoint>[m
[32m+[m[32m                    <fes:ValueReference>sf:the_geom</fes:ValueReference>[m
[32m+[m[32m                    <!-- gml:id is mandatory on GML 3.2 geometry elements -->[m
[32m+[m[32m                    <gml:Polygon gml:id="polygon.1"[m
[32m+[m[32m                        srsName='http://www.opengis.net/def/crs/EPSG/0/26713'>[m
[32m+[m[32m                        <gml:exterior>[m
[32m+[m[32m                            <gml:LinearRing>[m
[32m+[m[32m                                <!-- pairs must form a closed ring -->[m
[32m+[m[32m                                <gml:posList>590431 4915204 590430[m
[32m+[m[32m                                    4915205 590429 4915204 590430[m
[32m+[m[32m                                    4915203 590431 4915204</gml:posList>[m
[32m+[m[32m                            </gml:LinearRing>[m
[32m+[m[32m                        </gml:exterior>[m
[32m+[m[32m                    </gml:Polygon>[m
[32m+[m[32m                </fes:Disjoint>[m
[32m+[m[32m            </fes:Not>[m
[32m+[m[32m        </fes:Filter>[m
[32m+[m[32m    </wfs:Query>[m
[32m+[m[32m</wfs:GetFeature>[m
[1mdiff --git a/src/geoserver/data/demo/WFS_mathGetFeature.url b/src/geoserver/data/demo/WFS_mathGetFeature.url[m
[1mnew file mode 100644[m
[1mindex 0000000..4dab41d[m
