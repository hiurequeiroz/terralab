[36m@@ -0,0 +1 @@[m
[32m+[m[32mwfs?request=GetFeature&version=1.1.0&typeName=topp:states&formatName=GML2&FILTER=%3Cogc:Filter%20xmlns:ogc=%22http://www.opengis.net/ogc%22%3E%3Cogc:PropertyIsGreaterThan%3E%3Cogc:Div%3E%3Cogc:PropertyName%3EMANUAL%3C/ogc:PropertyName%3E%3Cogc:PropertyName%3EWORKERS%3C/ogc:PropertyName%3E%3C/ogc:Div%3E%3Cogc:Literal%3E0.25%3C/ogc:Literal%3E%3C/ogc:PropertyIsGreaterThan%3E%3C/ogc:Filter%3E[m
\ No newline at end of file[m
[1mdiff --git a/src/geoserver/data/demo/WFS_mathGetFeature.xml b/src/geoserver/data/demo/WFS_mathGetFeature.xml[m
[1mnew file mode 100644[m
[1mindex 0000000..84b2f60[m
[1m--- /dev/null[m
[1m+++ b/src/geoserver/data/demo/WFS_mathGetFeature.xml[m
[36m@@ -0,0 +1,26 @@[m
[32m+[m[32m<!-- Example of a getFeature with a math filter.  In this case  -->[m
[32m+[m[32m<!-- we want the states that have over 25% of their workers -->[m
[32m+[m[32m<!-- doing manual labor.  So we divide the manual field by    -->[m
[32m+[m[32m<!-- the total number of workers and compare it against 0.25 -->[m
[32m+[m
[32m+[m[32m<wfs:GetFeature service="WFS" version="1.0.0"[m
[32m+[m[32m  outputFormat="GML2"[m
[32m+[m[32m  xmlns:topp="http://www.openplans.org/topp"[m
[32m+[m[32m  xmlns:wfs="http://www.opengis.net/wfs"[m
[32m+[m[32m  xmlns:ogc="http://www.opengis.net/ogc"[m
[32m+[m[32m  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"[m
[32m+[m[32m  xsi:schemaLocation="http://www.opengis.net/wfs[m
[32m+[m[32m                      http://schemas.opengis.net/wfs/1.0.0/WFS-basic.xsd">[m
[32m+[m[32m  <wfs:Query typeName="topp:states">[m
[32m+[m[32m    <ogc:Filter>[m
[32m+[m[32m      <ogc:PropertyIsGreaterThan>[m
[32m+[m[32m        <ogc:Div>[m
[32m+[m[32m            <ogc:PropertyName>MANUAL</ogc:PropertyName>[m
[32m+[m[32m            <ogc:PropertyName>WORKERS</ogc:PropertyName>[m
[32m+[m[32m        </ogc:Div>[m
[32m+[m[32m         <ogc:Literal>0.25</ogc:Literal>[m
[32m+[m[32m    </ogc:PropertyIsGreaterThan>[m
[32m+[m[32m    </ogc:Filter>[m
[32m+[m[32m    </wfs:Query>[m
[32m+[m[32m</wfs:GetFeature>[m
[32m+[m[41m [m
[1mdiff --git a/src/geoserver/data/demo/WFS_transactionDelete.xml b/src/geoserver/data/demo/WFS_transactionDelete.xml[m
[1mnew file mode 100644[m
